{
  "version": "0.1.0",
  "name": "detect-pullups",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "start": "tsdx watch",
    "build": "tsdx build",
    "rollup": "yarn clean && rollup -c",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "description": "Detect pullups from timeseries data",
  "author": "Adam Tombleson",
  "module": "dist/detect-pullups.esm.js",
  "size-limit": [
    {
      "path": "dist/detect-pullups.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/detect-pullups.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "@types/lodash": "^4.14.168",
    "esbuild": "^0.11.6",
    "husky": "^6.0.0",
    "rollup": "^2.44.0",
    "rollup-plugin-dts": "^3.0.1",
    "rollup-plugin-esbuild": "^3.0.2",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "slayer": "^1.0.1"
  },
  "peerDependencies": {
  }
}
