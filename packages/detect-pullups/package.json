{
  "version": "0.1.6",
  "name": "detect-pullups",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "files": [
    "dist/**"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc -w",
    "build": "tsc",
    "start": "tsdx watch",
    "build:old": "tsdx build",
    "rollup": "yarn clean && rollup -c",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "-prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "description": "Detect peak and dips etc from timeseries data",
  "author": "Adam Tombleson",
  "size-limit": [
    {
      "path": "dist/detect-pullups.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/detect-pullups.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "@types/lodash-es": "^4.14.168",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "size-limit": "^4.10.2",
    "tsconfig": "*",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "react-is": "^17.0.2",
    "slayer": "*"
  },
  "private": true
}
