{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": [
        "coverage/**"
      ]
    },
    "database#test": {
      "dependsOn": ["detect-pullups#build"]
    },
    "database#build": {
      "dependsOn": ["detect-pullups#build"]
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}